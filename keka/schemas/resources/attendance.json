{
    "definition": {
        "url": "https://{{secrets.company}}.{{secrets.environment}}.com/api/v1/hris/jobtitles",
        "method": "get",
        "headers": {
            "Authorization": "Bearer {{secrets.access_token}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.errors}}"
            },
            "output": "{{response.data.data}}"
        },
        "pagination": {
            "qs": {
                "limit": 20
            },
            "type": "url",
            "default": {
                "limit": 20,
                "next_url": null
            },
            "next_url": "{{response.data.nextPage}}",
            "condition": "{{response.data.nextPage}} !== null"
        }
    },
    "parameters": []
}
