    "parameters": [
        {
            "meta": {
                "options": [
                    {
                        "label": "Split PDF",
                        "value": "pdf_split.pdf_split_by_pages",
                        "description": "Split PDF."
                    },
                    {
                        "label": "Split PDF by Text Search",
                        "value": "pdf_split.pdf_split_by_text",
