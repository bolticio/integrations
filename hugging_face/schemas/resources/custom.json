    "parameters": [
        {
            "meta": {
                "options": [
                    {
                        "label": "Custom API",
                        "value": "custom.custom_api"
                    }
                ],
                "validation": {
                    "required": true
                },
                "description": "Select the operation you want to perform.",
                "displayName": "Operation",
                "displayType": "select",
                "placeholder": "Select an operation",
                "dependencies": {
                    "conditions": [
                        {
