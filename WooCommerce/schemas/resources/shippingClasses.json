{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The name of the shipping class (e.g., 'Priority').",
                "displayName": "Name",
                "displayType": "text",
                "placeholder": "Enter shipping class name."
            },
            "name": "name"
        }
    ],
    "definition": {
        "qs": {
            "consumer_key": "{{secrets.consumer_key}}",
            "consumer_secret": "{{secrets.consumer_secret}}"
        },
        "url": "https://{{secrets.store_domain}}/wp-json/wc/v3/products/shipping_classes",
        "body": {
            "name": "{{parameters.name}}"
        },
        "method": "post",
        "headers": {
            "Content-Type": "application/json"
        },
        "response": {
            "error": {
                "code": "{{response.data.data.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
