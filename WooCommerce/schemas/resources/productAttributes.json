{
    "parameters": [
        {
            "meta": {
                "config": {
                    "url": "integrations/options",
                    "body": {
                        "name": "WooCommerce",
                        "secret": "{{parameters.secret}}",
                        "resource": "productAttributes",
                        "operation": "productAttributes.get",
                        "loadOptionsMethod": "list"
                    },
                    "method": "post",
                    "urlType": "options",
                    "labelKey": "name",
                    "valueKey": "id"
                },
                "options": [],
                "validation": {
                    "required": true
                },
                "description": "Name of the product attribute to retrieve.",
                "displayName": "Attribute Name",
                "displayType": "text",
                "placeholder": "Enter attribute Name"
            },
            "name": "attribute_id"
        }
    ],
    "definition": {
        "qs": {
            "consumer_key": "{{secrets.consumer_key}}",
            "consumer_secret": "{{secrets.consumer_secret}}"
        },
        "url": "https://{{secrets.store_domain}}/wp-json/wc/v3/products/attributes/{{parameters.attribute_id}}",
        "method": "get",
        "headers": {
            "Content-Type": "application/json"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
