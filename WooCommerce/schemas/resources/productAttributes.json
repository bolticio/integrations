{
    "definition": {
        "qs": {
            "consumer_key": "{{secrets.consumer_key}}",
            "consumer_secret": "{{secrets.consumer_secret}}"
        },
        "url": "https://{{secrets.store_domain}}/wp-json/wc/v3/orders/{{parameters.order_id}}/notes/{{parameters.note_id}}",
        "method": "get",
        "headers": {
            "Content-Type": "application/json"
        },
        "response": {
            "error": {
                "code": "{{response.data.data}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    },
    "parameters": [
        {
            "meta": {
                "config": {
                    "url": "integrations/options",
                    "body": {
                        "name": "WooCommerce",
                        "secret": "{{parameters.secret}}",
                        "resource": "orderNote",
                        "operation": "orderNote.get",
                        "loadOptionsMethod": "getOrder"
                    },
                    "method": "post",
                    "urlType": "options",
                    "labelKey": "id",
                    "valueKey": "id"
                },
                "options": [],
                "validation": {
                    "required": true
                },
                "description": "The unique identifier of the order.",
                "displayName": "Order ID",
                "displayType": "select",
                "placeholder": "Enter Order ID"
            },
            "name": "order_id"
        },
        {
            "meta": {
                "config": {
                    "url": "integrations/options",
                    "body": {
                        "name": "WooCommerce",
                        "secret": "{{parameters.secret}}",
                        "order_id": "{{parameters.order_id}}",
                        "resource": "orderNote",
                        "operation": "orderNote.get",
                        "loadOptionsMethod": "list"
                    },
                    "method": "post",
                    "urlType": "options",
                    "labelKey": "id",
                    "valueKey": "id",
                    "condition": "{{parameters.order_id}}"
                },
                "options": [],
                "validation": {
                    "required": true
                },
                "description": "The unique identifier of the note.",
                "displayName": "Note ID",
                "displayType": "select",
                "placeholder": "Enter Note ID"
            },
            "name": "note_id"
        }
    ]
}
