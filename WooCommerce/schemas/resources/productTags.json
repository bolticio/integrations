{
    "parameters": [
        {
            "meta": {
                "config": {
                    "url": "integrations/options",
                    "body": {
                        "name": "WooCommerce",
                        "secret": "{{parameters.secret}}",
                        "resource": "productTags",
                        "operation": "productTags.delete",
                        "loadOptionsMethod": "list"
                    },
                    "method": "post",
                    "urlType": "options",
                    "labelKey": "name",
                    "valueKey": "id"
                },
                "options": [],
                "validation": {
                    "required": true
                },
                "description": "The Name of the product tag you want to delete.",
                "displayName": "Product Tag Name",
                "displayType": "select",
                "placeholder": "Enter Product Tag Name"
            },
            "name": "product_tag_id"
        }
    ],
    "definition": {
        "qs": {
            "consumer_key": "{{secrets.consumer_key}}",
            "consumer_secret": "{{secrets.consumer_secret}}"
        },
        "url": "https://{{secrets.store_domain}}/wp-json/wc/v3/products/tags/{{parameters.product_tag_id}}?force=true",
        "method": "delete",
        "response": {
            "error": {
                "code": "{{response.data.data.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
