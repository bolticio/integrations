{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The ID of the tax rate to delete.",
                "displayName": "Tax Rate ID",
                "displayType": "text",
                "placeholder": "Enter tax rate ID"
            },
            "name": "id"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Whether to forcefully delete the tax rate.",
                "displayName": "Force Delete",
                "displayType": "toggle",
                "placeholder": "Enable/Disable force delete"
            },
            "name": "force"
        }
    ],
    "definition": {
        "qs": {
            "consumer_key": "{{secrets.consumer_key}}",
            "consumer_secret": "{{secrets.consumer_secret}}"
        },
        "url": "https://{{secrets.store_domain}}/wp-json/wc/v3/taxes/{{parameters.id}}?force=true",
        "method": "delete",
        "response": {
            "error": {
                "code": "{{response.data.data.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
