{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The SKU of the product to check salability for.",
                "displayName": "Product SKU",
                "displayType": "text",
                "placeholder": "Enter the product SKU"
            },
            "name": "sku"
        },
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The ID of the stock to check against.",
                "displayName": "Stock ID",
                "displayType": "number",
                "placeholder": "Enter the stock ID"
            },
            "name": "stockId"
        },
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The quantity to check for salability.",
                "displayName": "Requested Quantity",
                "displayType": "number",
                "placeholder": "Enter the requested quantity"
            },
            "name": "requestedQty"
        }
    ],
    "getDistance": {
        "definition": {
            "qs": {
                "origins": "{{parameters.origins}}",
                "destinations": "{{parameters.destinations}}"
            },
            "url": "https://{{secrets.domain}}/rest/V1/inventory/get-distance",
            "method": "get",
            "headers": {
                "Authorization": "Bearer {{secrets.access_token}}"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.message}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": [
            {
                "meta": {
                    "validation": {
                        "required": true
                    },
                    "description": "Comma-separated list of origin addresses.",
                    "displayName": "Origins",
                    "displayType": "text",
                    "placeholder": "Enter origin addresses"
                },
                "name": "origins"
            },
            {
                "meta": {
                    "validation": {
                        "required": true
                    },
                    "description": "Comma-separated list of destination addresses.",
                    "displayName": "Destinations",
                    "displayType": "text",
                    "placeholder": "Enter destination addresses"
                },
                "name": "destinations"
            }
        ]
    },
