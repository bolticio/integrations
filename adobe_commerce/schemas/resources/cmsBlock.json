{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The ID of the CMS block to delete.",
                "displayName": "Block ID",
                "displayType": "text",
                "placeholder": "Enter the Block ID"
            },
            "name": "id"
        }
    ],
    "definition": {
        "url": "https://{{secrets.domain}}/rest/default/V1/cmsBlock/{{parameters.id}}",
        "method": "delete",
        "headers": {
            "Authorization": "Bearer {{secrets.access_token}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
