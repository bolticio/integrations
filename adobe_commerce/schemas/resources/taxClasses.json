{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "The Class ID to check against.",
                "displayName": "Class ID",
                "displayType": "number",
                "placeholder": "Enter the Class ID"
            },
            "name": "classId"
        }
    ],
    "getTaxClasses": {
        "definition": {
            "url": "https://{{secrets.domain}}/rest/default/V1/taxClasses",
            "method": "get",
            "headers": {
                "Authorization": "Bearer {{secrets.access_token}}"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.message}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": []
    },
    "getTaxClassByTaxClassId": {
        "definition": {
            "url": "https://{{secrets.domain}}/rest/default/V1/taxClasses/{taxClassId}",
            "method": "get",
            "headers": {
                "Authorization": "Bearer {{secrets.access_token}}"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.message}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": []
    },
    "definition": {
        "url": "https://{{secrets.domain}}/rest/default/V1/taxClasses/{{parameters.classId}}",
        "method": "get",
        "headers": {
            "Authorization": "Bearer {{secrets.access_token}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
