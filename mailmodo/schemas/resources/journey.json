{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "Enter the journey_id",
                "displayName": "Journey Id",
                "displayType": "text",
                "placeholder": "Enter the journey_id"
            },
            "name": "journey_id"
        },
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "Enter the email to start the journey.",
                "displayName": "Email",
                "displayType": "text",
                "placeholder": "Enter the email"
            },
            "name": "email"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Enter additional data for the journey.",
                "displayName": "Data",
                "displayType": "textarea",
                "placeholder": "Enter additional data"
            },
            "name": "data"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Allow duplicate email addresses for the journey.",
                "displayName": "Allow Duplicate Email",
                "displayType": "toggle"
            },
            "name": "allowDuplicateEmail"
        }
    ],
    "definition": {
        "qs": {},
        "url": "https://api.mailmodo.com/hooks/start/{{parameters.journey_id}}",
        "body": "{{parameters}}",
        "method": "post",
        "headers": {
            "Accept": "application/json",
            "mmApiKey": "{{secrets.api_key}}",
            "Content-Type": "application/json"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
