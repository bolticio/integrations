    "getTimezone": {
        "definition": {
            "qs": {},
            "url": "https://worldtimeapi.org/api/timezone",
            "method": "get",
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.error}}"
                },
                "output": "{{response.data}}"
            },
            "transform": {
                "output": "const result = output.map((timezone) => { return { label: timezone, value: timezone }; }); return result;"
            }
        }
    },
    "custom_api": {
