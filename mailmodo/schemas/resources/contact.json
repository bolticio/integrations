{
    "parameters": [],
    "getTimezone": {
        "definition": {
            "qs": {},
            "url": "https://worldtimeapi.org/api/timezone",
            "method": "get",
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.error}}"
                },
                "output": "{{response.data}}"
            },
            "transform": {
                "output": "const result = output.map((timezone) => { return { label: timezone, value: timezone }; }); return result;"
            }
        }
    },
    "definition": {
        "qs": {},
        "url": "https://api.mailmodo.com/api/v1/getAllContactLists",
        "method": "get",
        "headers": {
            "Accept": "application/json",
            "mmApiKey": "{{secrets.api_key}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
