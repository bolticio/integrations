{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "Enter the email for the event.",
                "displayName": "Email",
                "displayType": "text",
                "placeholder": "Enter the email"
            },
            "name": "email"
        },
        {
            "meta": {
                "validation": {
                    "required": true
                },
                "description": "Enter the name of the event.",
                "displayName": "Event Name",
                "displayType": "text",
                "placeholder": "Enter the event name"
            },
            "name": "event_name"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Enter the timestamp for the event.",
                "displayName": "Timestamp",
                "displayType": "number",
                "placeholder": "Enter the timestamp"
            },
            "name": "ts"
        },
        {
            "meta": {
                "children": [
                    {
                        "meta": {
                            "validation": {
                                "required": true
                            },
                            "displayName": "Key",
                            "displayType": "text"
                        },
                        "name": "key"
                    },
                    {
                        "meta": {
                            "validation": {
                                "required": true
                            },
                            "displayName": "Value",
                            "displayType": "text"
                        },
                        "name": "value"
                    }
                ],
                "readOnly": false,
                "htmlProps": {
                    "allowAdd": true,
                    "allowDynamic": true
                },
                "validation": {
                    "max": 5
                },
                "displayName": "Event Properties",
                "displayType": "keyvalue"
            },
            "name": "event_properties"
        }
    ],
    "definition": {
        "qs": {},
        "url": "https://api.mailmodo.com/api/v1/addEvent",
        "body": "{{parameters}}",
        "method": "post",
        "headers": {
            "Accept": "application/json",
            "mmApiKey": "{{secrets.api_key}}",
            "Content-Type": "application/json"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
