{
    "api_key": {
        "validate": {
            "url": "https://api.firecrawl.dev/v1/crawl/active",
            "method": "get",
            "headers": {
                "Content-Type": "application/json",
                "Authorization": "Bearer {{secrets.api_key}}"
            },
            "response": {
                "error": {
                    "code": "{{response.code}}",
                    "message": "{{response.data.message}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": [
            {
                "meta": {
                    "validation": {
                        "required": true,
                        "requiredDetail": {
                            "errorMsg": "API key is required"
                        }
                    },
                    "description": "You can go to <a href='https://www.firecrawl.dev/app' target='_blank'>Firecrawl dev dashboard </a> to generate an API key.",
                    "displayName": "API Key",
                    "displayType": "password",
                    "placeholder": "Enter API Key"
                },
                "name": "api_key"
            }
        ]
    },
    "parameters": [
        {
            "meta": {
                "value": "api_key",
                "options": [
                    {
                        "label": "API Key",
                        "value": "api_key"
                    }
                ],
                "displayName": "Authentication Type",
                "displayType": "hidden"
            },
            "name": "type"
        }
    ]
}
