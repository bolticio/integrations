{
    "parameters": [
        {
            "meta": {
                "htmlProps": {
                    "maxlength": 50
                },
                "validation": {
                    "required": true,
                    "requiredDetail": {
                        "errorMsg": "Generation ID is a required field."
                    }
                },
                "description": "The unique identifier for a generation request from the Boltic AI. Use this ID to fetch the generated result.",
                "displayName": "Generation ID",
                "displayType": "text",
                "placeholder": "Enter the generation ID"
            },
            "name": "generationId"
        }
    ],
    "definition": {
        "qs": {
            "id": "{{parameters.generationId}}"
        },
        "url": "https://openrouter.ai/api/v1/generation",
        "body": {},
        "method": "get",
        "headers": {
            "Content-Type": "application/json",
            "Authorization": "Bearer {{secrets.api_key}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.error.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
