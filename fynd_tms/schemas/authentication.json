{
    "api_key": {
        "validate": {
            "url": "https://api.scm.fynd.com/tms/service/public/hyperlocal/v1.0/shipments?column=awb_number",
            "method": "get",
            "headers": {
                "Content-Type": "application/json",
                "account-token": "{{secrets.api_key}}"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.errors[0].msg}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": [
            {
                "meta": {
                    "validation": {
                        "required": true,
                        "requiredDetail": {
                            "errorMsg": "API key is required field."
                        }
                    },
                    "description": "Refer to the <a href='https://api.scm.fynd.com/tms/service/public/hyperlocal/redoc#tag/Shipment-v1.0/operation/generate_shipping_label' target='blank'>Fynd TMS authenticaton documentation</a> for detailed instructions on getting your API key.",
                    "displayName": "API Key",
                    "displayType": "password",
                    "placeholder": "Enter API key."
                },
                "name": "api_key"
            }
        ]
    },
    "parameters": [
        {
            "meta": {
                "value": "api_key",
                "options": [
                    {
                        "label": "API Key",
                        "value": "api_key"
                    }
                ],
                "validation": {
                    "required": true
                },
                "description": "Choose the type of authentication you want to use.",
                "displayName": "Authentication Type",
                "displayType": "hidden",
                "placeholder": "Select an authentication type"
            },
            "name": "type"
        }
    ]
}
