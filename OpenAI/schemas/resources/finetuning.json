{
    "name": "finetuning",
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true,
                    "requiredDetail": {
                        "errorMsg": "Fine Tuning Job ID is required field"
                    }
                },
                "description": "The ID of the fine-tuning job to get events for.",
                "displayName": "Fine Tuning Job ID",
                "displayType": "text",
                "placeholder": "Enter a fine tuning job id"
            },
            "name": "fineTuningJobId"
        },
        {
            "meta": {
                "value": 20,
                "validation": {
                    "required": false
                },
                "description": "Number of events to retrieve.",
                "displayName": "Limit",
                "displayType": "number",
                "placeholder": "Enter a limit (number of events)"
            },
            "name": "limit"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Identifier for the last event from the previous pagination request.",
                "displayName": "After",
                "displayType": "text",
                "placeholder": "Enter after text"
            },
            "name": "after"
        }
    ],
    "definition": {
        "qs": {
            "after": "{{parameters.after}}",
            "limit": "{{parameters.limit}}"
        },
        "url": "https://api.openai.com/v1/fine_tuning/jobs/{{parameters.fineTuningJobId}}/events",
        "method": "get",
        "headers": {
            "Authorization": "Bearer {{secrets.api_token}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.error.message}}"
            },
            "output": "{{response.data}}"
        }
    }
}
