{
    "getModels": {
        "definition": {
            "url": "https://app.nanonets.com/api/v2/ImageCategorization/Models/",
            "method": "get",
            "headers": {
                "Authorization": "Basic convertBufferBase64String({{secrets.api_token}}:X)"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.message}}"
                },
                "output": "{{response.data}}"
            }
        }
    },
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": true,
                    "requiredDetail": {
                        "errorMsg": "Category is required field."
                    }
                },
                "description": "This will be added to the description of the model (Eg. invoice, tables)",
                "displayName": "Category",
                "displayType": "multitext",
                "placeholder": "Enter the Categories"
            },
            "name": "category"
        }
    ],
    "definition": {
        "url": "https://app.nanonets.com/api/v2/OCR/Model/",
        "body": "{{parameters}}",
        "method": "post",
        "headers": {
            "Content-Type": "application/json",
            "Authorization": "Basic convertBufferBase64String({{secrets.api_token}}:X)"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data.message}}"
            },
            "output": "{{response.data}}"
        },
        "transform": {
            "body": "const data = {'categories': '{{parameters.category}}'}; return data;"
        }
    }
}
