{
    "parameters": [
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Comma-separated list of notification destination IDs (ntfset_*).",
                "displayName": "Notification Setting ID",
                "displayType": "text",
                "placeholder": "ntfset_01gsz8z1q1n00f12qt82y31smh,ntfset_01gsz8z1q1n00f12qt82y31abc"
            },
            "name": "notification_setting_id"
        },
        {
            "meta": {
                "options": [
                    {
                        "label": "ID (ascending)",
                        "value": "id[ASC]"
                    },
                    {
                        "label": "ID (descending)",
                        "value": "id[DESC]"
                    }
                ],
                "htmlProps": {
                    "allowDynamic": true
                },
                "validation": {
                    "required": false
                },
                "description": "Order returned entities (only `id` is supported).",
                "displayName": "Order By",
                "displayType": "select",
                "placeholder": "Select ordering"
            },
            "name": "order_by"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Free-text search across `id` and `type` fields.",
                "displayName": "Search",
                "displayType": "text",
                "placeholder": "subscription.paused"
            },
            "name": "search"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Comma-separated list of status values (e.g., delivered, failed).",
                "displayName": "Status",
                "displayType": "text",
                "placeholder": "delivered,failed"
            },
            "name": "status"
        },
        {
            "meta": {
                "validation": {
                    "required": false
                },
                "description": "Return notifications that reference this Paddle ID (txn_*, ctm_*, sub_*).",
                "displayName": "Filter",
                "displayType": "text",
                "placeholder": "txn_01gsz8z1q1n00f12qt82y31smh"
            },
            "name": "filter"
        },
        {
            "meta": {
                "readOnly": false,
                "htmlProps": {
                    "format": "YYYY-MM-DD HH:mm:ss",
                    "disabled": false,
                    "timeSteps": {
                        "hours": 1,
                        "minutes": 5,
                        "seconds": 5
                    }
                },
                "validation": {
                    "required": false
                },
                "description": "RFC 3339 datetime to start the range.",
                "displayName": "From",
                "displayType": "datetime",
                "placeholder": "2024-01-01T00:00:00Z"
            },
            "name": "from"
        },
        {
            "meta": {
                "readOnly": false,
                "htmlProps": {
                    "format": "YYYY-MM-DD HH:mm:ss",
                    "disabled": false,
                    "timeSteps": {
                        "hours": 1,
                        "minutes": 5,
                        "seconds": 5
                    }
                },
                "validation": {
                    "required": false
                },
                "description": "RFC 3339 datetime to end the range.",
                "displayName": "To",
                "displayType": "datetime",
                "placeholder": "2024-12-31T23:59:59Z"
            },
            "name": "to"
        }
    ],
    "definition": {
        "qs": {
            "to": "{{parameters.to}}",
            "from": "{{parameters.from}}",
            "filter": "{{parameters.filter}}",
            "search": "{{parameters.search}}",
            "status": "{{parameters.status}}",
            "order_by": "{{parameters.order_by}}",
            "notification_setting_id": "{{parameters.notification_setting_id}}"
        },
        "url": "https://{{secrets.environment_url}}/notifications",
        "method": "get",
        "headers": {
            "Accept": "application/json",
            "Authorization": "Bearer {{secrets.api_key}}"
        },
        "response": {
            "error": {
                "code": "{{response.status}}",
                "message": "{{response.data}}"
            },
            "output": "{{response.data.data}}"
        },
        "pagination": {
            "qs": {
                "per_page": 200
            },
            "type": "url",
            "default": {
                "next_url": null,
                "per_page": 200
            },
            "next_url": "{{response.data.meta.pagination.next}}",
            "condition": "{{response.data.meta.pagination.has_more}} !== false"
        }
    }
}
