{
    "parameters": [
        {
            "meta": {
                "options": [
                    {
                        "label": "Search",
                        "value": "google_search.search",
                        "description": "Google Programmable Search Engine API lets you add custom Google-powered search to your site or app, focusing on specific domains with advanced filtering and ranking."
                    }
                ],
                "validation": {
                    "required": true
                },
                "description": "Select the operation you want to perform.",
                "displayName": "Operation",
                "displayType": "select",
                "placeholder": "Select an operation",
                "dependencies": {
                    "conditions": [
                        {
                            "field": "resource",
                            "value": "google_search",
                            "operator": "EQUALS"
                        }
                    ]
                }
            },
            "name": "operation"
        }
    ],
    "search": {
        "definition": {
            "qs": {
                "q": "{{parameters.query}}",
                "cx": "{{secrets.search_engine_id}}",
                "key": "{{secrets.api_key}}"
            },
            "url": "https://www.googleapis.com/customsearch/v1",
            "method": "get",
            "headers": {
                "ContentType": "application/json"
            },
            "response": {
                "error": {
                    "code": "{{response.data.errors.code}}",
                    "message": "{{response.data.errors.message}}"
                },
                "output": "{{response.data}}"
            }
        },
        "parameters": [
            {
                "meta": {
                    "validation": {
                        "required": true
                    },
                    "description": "Enter the search query you want to google.",
                    "displayName": "Search Query",
                    "displayType": "textarea",
                    "placeholder": "Enter Search Query"
                },
                "name": "query"
            }
        ]
    }
}
