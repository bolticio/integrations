    "update": {
        "definition": {
            "url": "https://api.monday.com/v2",
            "body": {},
            "method": "post",
            "headers": {
                "Content-Type": "application/json",
                "Authorization": "{{secrets.access_token}}"
            },
            "response": {
                "error": {
                    "code": "{{response.status}}",
                    "message": "{{response.data.errors[0].message}}"
                },
                "output": "{{response.data}}"
            },
            "transform": {
                "body": "const itemId = {{parameters.subitem_id}}; const boardId = {{parameters.board_id}}; const STATUS = 'status', DATE = 'date0', PERSON = 'person'; let columnValues = {}; if ('{{parameters.status}}' !== 'undefined') columnValues[STATUS] = { index: {{parameters.status}} }; if ('{{parameters.date}}' !== 'undefined') columnValues[DATE] = { date: '{{parameters.date}}' }; if ('{{parameters.person}}' !== 'undefined') columnValues[PERSON] = { personsAndTeams: [{ id: {{parameters.person}}, kind: \"person\" }] }; const columnValuesString = JSON.stringify(columnValues).replace(/\"/g, '\\\\\\\"'); return JSON.stringify({ query: `mutation { change_multiple_column_values ( item_id: ${itemId}, board_id: ${boardId}, column_values: \\\"${columnValuesString}\\\" ) { id } }` });"
            }
        },
        "parameters": [
            {
                "meta": {
                    "config": {
                        "url": "integrations/options",
                        "body": {
                            "name": "monday",
                            "secret": "{{parameters.secret}}",
                            "resource": "subitem",
                            "operation": "subitem.update",
                            "loadOptionsMethod": "getBoard"
                        },
                        "method": "post",
                        "urlType": "options",
                        "labelKey": "name",
                        "valueKey": "id"
                    },
                    "options": [],
                    "htmlProps": {
                        "allowDynamic": true
                    },
                    "validation": {
                        "required": true
                    },
                    "description": "Select the board of subitem which needs to be updated.",
                    "displayName": "Board",
                    "displayType": "select",
                    "placeholder": "Select a board"
                },
                "name": "board_id"
            },
            {
                "meta": {
                    "config": {
                        "url": "integrations/options",
                        "body": {
                            "name": "monday",
                            "secret": "{{parameters.secret}}",
                            "board_id": "{{parameters.board_id}}",
                            "resource": "subitem",
                            "operation": "subitem.update",
                            "loadOptionsMethod": "getBoardItems"
                        },
                        "method": "post",
                        "urlType": "options",
                        "labelKey": "name",
                        "valueKey": "id",
                        "condition": "{{parameters.board_id}}"
                    },
                    "options": [],
                    "htmlProps": {
                        "allowDynamic": true
                    },
                    "validation": {
                        "required": true
                    },
                    "description": "Select the subitem to be updated.",
                    "displayName": "Subitem",
                    "displayType": "select",
                    "placeholder": "Select an subitem",
