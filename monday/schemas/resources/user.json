                "name": "subitem_id"
            },
            {
                "meta": {
                    "config": {
                        "url": "integrations/options",
                        "body": {
                            "name": "monday",
                            "secret": "{{parameters.secret}}",
                            "resource": "subitem",
                            "operation": "subitem.update",
                            "loadOptionsMethod": "getUsers"
                        },
                        "method": "post",
                        "urlType": "options",
                        "labelKey": "name",
                        "valueKey": "id"
                    },
                    "options": [],
                    "htmlProps": {
                        "allowDynamic": true
                    },
                    "validation": {
                        "required": false
                    },
                    "description": "Select new owner of the subitem.",
                    "displayName": "Owner",
                    "displayType": "select",
                    "placeholder": "Select owner of the subitem",
                    "dependencies": {
